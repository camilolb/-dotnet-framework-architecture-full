        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="ndfdXMLBinding">
                    <security mode="Transport" />
                </binding>
                <binding name="ndfdXMLBinding1" />
